{
  "name": "ol-vue",
  "version": "0.0.9",
  "description": "openLayers for vue ne sxt",
  "main": "dist/ol-vue.js",
  "browser": "dist/ol-vue.js",
  "module": "dist/ol-vue.js",
  "unpkg": "dist/ol-vue.min.js",
  "types": "dist/ol-vue.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup -c --format es",
    "lint": "eslint ./src --ext=.vue,.ts,.js,.json --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owenvip/ol-vue.git"
  },
  "keywords": [
    "ol",
    "openLayers",
    "vue",
    "ts",
    "typescript"
  ],
  "author": "owenwong <owen.cq.cn@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/owenvip/ol-vue/issues"
  },
  "homepage": "https://github.com/owenvip/ol-vue#readme",
  "dependencies": {
    "ol": "^6.6.1",
    "ol-contextmenu": "^4.1.0",
    "ol-ext": "^3.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@otools/compile-tools": "^0.0.15",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.1.2",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.2.3",
    "rollup": "^2.36.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0"
  },
  "peerDependencies": {
    "typescript": "^4.3.5",
    "vue": "^3.2.21"
  },
  "engines": {
    "node": ">=12"
  },
  "prettier": "@otools/prettier-config-otool",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/no-multiple-template-root": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  }
}
